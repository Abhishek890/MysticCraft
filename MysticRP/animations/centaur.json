        "leg2": { "rotation": [ "Math.cos(Params.AnimTime * 38.17) * -80.0", 0.0, 0.0 ] },
        "leg3": { "rotation": [ "Math.cos(Params.AnimTime * 38.17) *  80.0", 0.0, 0.0 ] },
        "leftArm": { "rotation": [ "Math.cos(Params.AnimTime * 38.17) * -80.0", 0.0, 0.0 ] },
        "rightArm": { "rotation": [ "Math.cos(Params.AnimTime * 38.17) *  80.0", 0.0, 0.0 ] }
      }
    }
  }

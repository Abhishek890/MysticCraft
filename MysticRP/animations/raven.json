{
	"format_version": "1.8.0",
	"animations": {
		"animation.raven.wings": {
			"anim_time_update": "query.walk_position",
			"loop": true,
			"bones": {
				"wing_a": {
					"rotation": [
						45,
						0,
						"110 + (Math.cos(global.anim_time * 360.0) *  30.0)"
					]
				},
				"wing_b": {
					"rotation": [
						45,
						0,
						"-110 + (-Math.cos(global.anim_time * 360.0) *  30.0)"
					]
				}
			}
		},
		"animation.raven.move": {
			"anim_time_update": "query.walk_position",
			"loop": true,
			"bones": {
				"leg_a": {
					"rotation": [
						"Math.cos(global.anim_time * 38.17) *  80.0",
						0,
						0
					]
				},
				"leg_b": {
					"rotation": [
						"Math.cos(global.anim_time * 38.17) * -80.0",
						0,
						0
					]
				}
			}
		}
	}
}